<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.guiclient.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="200.0">
      <children>
         <Label text="login:" />
         <TextField fx:id="loginField" />
      </children>
   </HBox>
   <Button fx:id="loginButton" onAction="#onLoginButtonClick" text="Login" />
   <Separator prefWidth="200.0" />
   <TextArea fx:id="outputArea" prefHeight="200.0" prefWidth="200.0" />
   <HBox alignment="TOP_CENTER" prefHeight="30.0" prefWidth="200.0">
      <children>
         <TextField fx:id="inputField" onAction="#onSendButtonClick" prefWidth="220.0" />
          <Button onAction="#onSendButtonClick" text="Send" />
      </children>
   </HBox>
   <Label text="online:" />
   <ListView fx:id="onlineList" prefHeight="200.0" prefWidth="100.0" />
</VBox>
